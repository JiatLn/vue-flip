{
  "name": "vue-flip",
  "version": "0.1.0",
  "author": {
    "name": "JiatLn",
    "email": "lne128gene980@163.com"
  },
  "license": "MIT",
  "keywords": [
    "vue",
    "animation",
    "flip"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "pkgroll --watch",
    "build": "pkgroll",
    "play": "vite playground --open",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@iconify/json": "^2.1.73",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.0.3",
    "@unocss/reset": "^0.43.2",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vueuse/core": "^8.9.0",
    "eslint": "^8.19.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "lodash-es": "^4.17.21",
    "pkgroll": "^1.3.1",
    "sass": "^1.53.0",
    "typescript": "^4.7.4",
    "unocss": "^0.43.2",
    "unplugin-auto-import": "^0.9.2",
    "unplugin-vue-components": "^0.21.1",
    "vite": "^2.9.13",
    "vue-tsc": "^0.38.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "src/**/*.ts": "eslint",
    "src/**/*.vue": "eslint"
  }
}
